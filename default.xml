<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <default sync-j="4" revision="jethro"/>

  <remote name="digi"  fetch="git://github.com/digi-embedded"/>
  <remote name="fsl"   fetch="git://github.com/Freescale"/>
  <remote name="qt5"   fetch="git://github.com/meta-qt5"/>
  <remote name="oe"    fetch="git://git.openembedded.org"/>
  <remote name="yocto" fetch="git://git.yoctoproject.org"/>

  <project name="meta-digi.git" path="sources/meta-digi" remote="digi" revision="jethro">
    <copyfile src="sdk/mkproject.sh" dest="mkproject.sh"/>
  </project>
  <project name="meta-fsl-arm.git"      path="sources/meta-fsl-arm"      remote="yocto" revision="jethro"/>
  <project name="meta-fsl-demos.git"    path="sources/meta-fsl-demos"    remote="fsl"   revision="jethro"/>
  <project name="meta-openembedded.git" path="sources/meta-openembedded" remote="oe"    revision="jethro"/>
  <project name="meta-qt5.git"          path="sources/meta-qt5"          remote="qt5"   revision="jethro"/>
  <project name="poky.git"              path="sources/poky"              remote="yocto" revision="jethro"/>
</manifest>
